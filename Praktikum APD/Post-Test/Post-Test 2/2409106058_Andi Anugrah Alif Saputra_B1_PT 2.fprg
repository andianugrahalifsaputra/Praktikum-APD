            <input variable="bb"/>
            <output expression="&quot;Apakah anda sudah menikah?&quot;" newline="True"/>
            <input variable="status"/>
            <assign variable="total" expression="usia+tb+bb"/>
            <if expression="status">
                <then>
                    <output expression="&quot;=================&quot;&#13;&#10;&amp;ToChar&#13;&#10;(13)&#13;&#10;&amp;&quot;Nama:&quot;&amp;nama&#13;&#10;&amp;ToChar&#13;&#10;(13)&#13;&#10;&amp;&quot;Tinggi Badan:&quot;&amp;tb&#13;&#10;&amp;ToChar&#13;&#10;(13)&#13;&#10;&amp;&quot;Berat Badan:&quot;&amp;bb&#13;&#10;&amp;ToChar&#13;&#10;(13)&#13;&#10;&amp;&quot;Status Pernikahan: Sudah Menikah&quot;&#13;&#10;&amp;ToChar&#13;&#10;(13)&#13;&#10;&amp;&quot;Total:&quot;&amp;total&#13;&#10;&amp;ToChar&#13;&#10;(13)&amp;&#13;&#10;&quot;=================&quot;" newline="True"/>
                </then>
                <else>
